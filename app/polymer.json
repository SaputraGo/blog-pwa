{
  "entrypoint": "index.html",
  "shell": "src/blog-pwa.html",
  "fragments": [
    "src/blog-static.html",
    "src/blog-chronicle.html",
    "src/lazy-resources.html"
  ],
  "sources": [
    "src/**/*",
    "images/**/*",
    "data/**/*",
    "helpers/**/*"
  ],
  "extraDependencies": [
    "push_manifest.json",
    "robots.txt",
    "manifest.json"
  ],
  "builds": [
    {
      "name": "default",
      "bundle": false,
      "js": { "compile": false, "minify": true },
      "css": { "minify": true },
      "html": { "minify": true },
      "addServiceWorker": true,
      "insertPrefetchLinks": true
    }
  ]
}